<resources>

    <!--  Allow CornerHandleView and PathSpecCornerPathRenderer to decouple from corner-radius -->
    <dimen name="config_rounded_mask_size">156px</dimen>

    <!-- the padding on the start of the statusbar -->
    <dimen name="status_bar_padding_start">0px</dimen>

    <!-- the padding on the end of the statusbar -->
    <dimen name="status_bar_padding_end">16px</dimen>

    <!-- the padding on the top of the statusbar (usually 0) -->
    <dimen name="status_bar_padding_top">16px</dimen>

    <!-- Height of the status bar header bar when on Keyguard -->
    <dimen name="status_bar_header_height_keyguard">@*android:dimen/status_bar_height</dimen>

    <!-- Margin on the left side of the carrier text on Keyguard -->
    <dimen name="keyguard_carrier_text_margin">@dimen/status_bar_padding_start</dimen>

    <!-- Location on the screen of the center of the physical power button. -->
    <dimen name="physical_power_button_center_screen_location_y">1000px</dimen>

    <!-- Alert Slider UI -->
    <dimen name="tri_state_down_dialog_position">520px</dimen>
    <dimen name="tri_state_down_dialog_position_l">270px</dimen>
    <dimen name="tri_state_middle_dialog_position">320px</dimen>
    <dimen name="tri_state_middle_dialog_position_l">@dimen/tri_state_down_dialog_position_l</dimen>
    <dimen name="tri_state_up_dialog_position">120px</dimen>
    <dimen name="tri_state_up_dialog_position_l">@dimen/tri_state_down_dialog_position_l</dimen>
    <dimen name="tri_state_up_dialog_position_deep">21px</dimen>
    <dimen name="tri_state_up_dialog_position_deep_land">@dimen/tri_state_up_dialog_position_deep</dimen>

</resources>
